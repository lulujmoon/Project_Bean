<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<!-- 바로 쓸 경우가 아니면 mapper는 미리 쓰지 말고 비워주세용 -->

<mapper namespace ="com.bb.bean.mail.MailDAO">

<select id="getList" resultType="MailDTO">
select * from mailAddr
</select>

<insert id="setInsert" parameterType="MailDTO">
insert into mailAddr values(#{mAddr})
</insert>
</mapper>
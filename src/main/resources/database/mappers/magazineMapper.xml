<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace ="com.bb.bean.magazine.MagazineDAO">

<insert id="magazineInsert" parameterType="MagazineDTO">
insert into magazine(num,title,subtitle,contents)values(magazine_seq.nextval,#{title},#{subTitle},#{contents})
</insert>

<select id= "magazineSelect" parameterType="MagazineDTO" resultType="MagazineDTO">
select * from magazine where num=#{num}
</select>

<select id= "magazineList" resultType="MagazineDTO">
select * from magazine
</select>




</mapper>